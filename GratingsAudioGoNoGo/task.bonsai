              <io:FileName>stimuli\gratings\2.csv</io:FileName>
              <io:ListSeparator>,</io:ListSeparator>
              <io:SkipRows>1</io:SkipRows>
            </Expression>
            <Expression xsi:type="scr:ExpressionTransform">
              <scr:Expression>new(
double.Parse(it[0]) * Math.PI / 180 as Orientation,
double.Parse(it[1]) as Speed,
double.Parse(it[2]) as Contrast,
double.Parse(it[3]) as Duration,
double.Parse(it[4]) as Angle
)</scr:Expression>
            </Expression>
            <Expression xsi:type="WorkflowOutput" />
